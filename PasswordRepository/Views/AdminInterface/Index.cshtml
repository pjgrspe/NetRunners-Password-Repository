<script type="text/javascript">
    function discardChangesADDITEM() {
        Swal.fire({
            title: 'Cancel changes?',
            text: 'This will dismiss all changes.',
            icon: 'error',
            iconColor: '#c40f2d',
            background: '#1a1a1a',
            showCancelButton: true,
            confirmButtonColor: '#70cc72',
            cancelButtonColor: '#fe4365',
            confirmButtonText: 'Confirm',
            cancelButtonText: 'Back'
        }).then((result) => {
            if (result.isConfirmed) {
                // Handle confirmed action
                $('#addbutton-modal').modal('hide')
            }
        });
    }

    function cancelEdit(PID, Username, Email, FName, Lname, PIN, Timeout) {
        //Confirmation message before cancelling edit
        Swal.fire({
            title: 'Cancel changes?',
            text: "This will dismiss all changes.",
            icon: 'error',
            iconColor: '#c40f2d',
            background: '#1a1a1a',
            showCancelButton: true,
            confirmButtonColor: '#70cc72',
            cancelButtonColor: '#fe4365',
            confirmButtonText: 'Confirm',
            cancelButtonText: 'Back'
        }).then((result) => {
            if (result.isConfirmed) {
                toggleEdit(PID);
                resetform(PID, Username, Email, FName, Lname, PIN, Timeout);
            }
        })

    }

    function resetform(PID, Username, Email, Fname, Lname, PIN, Timeout) {
        document.getElementById('Username-' + PID).value = Username;
        document.getElementById('Email-' + PID).value = Email;
        document.getElementById('Fname-' + PID).value = Fname;
        document.getElementById('Lname-' + PID).value = Lname;
        document.getElementById('PIN-' + PID).value = PIN;
        document.getElementById('Timeout-' + PID).value = Timeout;
    }

    function toggleEdit(PID) {
        var itemSwitch = document.getElementById('switch-' + PID);
        var itemUN = document.getElementById('Username-' + PID);
        var itemEMAIL = document.getElementById('Email-' + PID);
        var itemFname = document.getElementById('Fname-' + PID);
        var itemLname = document.getElementById('Lname-' + PID);
        var itemPIN = document.getElementById('PIN-' + PID);
        var itemICELock = document.getElementById('Timeout-' + PID);
        var editicon = document.getElementById('edit-menu-' + PID);
        var editbtns = document.getElementById('editbtns-' + PID);
        var dateinfo = document.getElementById('dateinfo-' + PID);

        //tbldetails
        const SwitchtblDetail = document.getElementById('activationswitch-' + PID)
        const UNtblDetail = document.getElementById('tbl-username-' + PID)
        const EMAILtblDetail = document.getElementById('tbl-email-' + PID)
        const FnametblDetail = document.getElementById('tbl-Fname-' + PID)
        const LnametblDetail = document.getElementById('tbl-Lname-' + PID)
        const PINtblDetail = document.getElementById('tbl-PIN-' + PID)
        const ICELocktblDetail = document.getElementById('tbl-TimeOut-' + PID)

        if (itemUN.value || itemPW.value || itemEMAIL.value || itemFName.value || itemLName.value || itemPIN.value || itemICELock.value != null) {
            //toggle
            if (itemUN.classList == 'form-control-plaintext readonly') {
                //REMOVE MENU ICONS
                editicon.style.display = "none";
                dateinfo.style.display = "none";
                //ADD CONFIRM AND CANCEL BUTTONS
                editbtns.style.display = "block";
                //ITEM SWITCH
                SwitchtblDetail.className = "settings";
                //ITEM UN
                itemUN.readOnly = false;
                itemUN.className = "form-control-edit";
                UNtblDetail.style.display = "block";
                //ITEM EMAIL
                itemEMAIL.readOnly = false;
                itemEMAIL.className = "form-control-edit";
                EMAILtblDetail.style.display = "block";
                //ITEM Fname
                itemFname.readOnly = false;
                itemFname.className = "form-control-edit";
                FnametblDetail.style.display = "block";
                //ITEM Lname
                itemLname.readOnly = false;
                itemLname.className = "form-control-edit";
                LnametblDetail.style.display = "block";
                //ITEM PIN
                itemPIN.readOnly = false;
                itemPIN.className = "form-control-edit";
                PINtblDetail.style.display = "block";
                itemPIN.type = 'password';
                //ITEM ICELock
                itemICELock.readOnly = false;
                itemICELock.className = "form-control-edit";
                ICELocktblDetail.style.display = "block";
            }
            else {
                //ADD MENU ICONS
                editicon.style.display = "block";
                dateinfo.style.display = "block";
                //REMOVE CONFIRM AND CANCEL BUTTONS
                editbtns.style.display = "none";
                //ITEM SWITCH
                SwitchtblDetail.className = "hidden";
                //ITEM UN
                itemUN.readOnly = true;
                itemUN.className = "form-control-plaintext readonly";
                //ITEM EMAIL
                itemEMAIL.readOnly = true;
                itemEMAIL.className = "form-control-plaintext";
                //ITEM Fname
                itemFname.readOnly = true;
                itemFname.className = "form-control-plaintext";
                //ITEM Lname
                itemLname.readOnly = true;
                itemLname.className = "form-control-plaintext";
                //ITEM PIN
                itemPIN.readOnly = true;
                itemPIN.className = "form-control-plaintext";
                itemPIN.type = 'password';
                //ITEM ICELock
                itemICELock.readOnly = true;
                itemICELock.className = "form-control-plaintext";
            }
        }
        else {
            Swal.fire({
                position: 'top',
                icon: 'error',
                iconColor: '#c40f2d',
                background: '#1a1a1a',
                title: 'Fill up all items!',
                showConfirmButton: false,
                timer: 2500
            })
        }

    }
</script>
