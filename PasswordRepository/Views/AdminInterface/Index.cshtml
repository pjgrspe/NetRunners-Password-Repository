        //Confirmation message before deleting entry
        Swal.fire({
            title: 'Ban User?',
            text: "This will permanently disable this user (Until an admin reactives this account)",
            icon: 'warning',
            iconColor: '#c40f2d',
            background: '#1a1a1a',
            showCancelButton: true,
            confirmButtonColor: '#70cc72',
            cancelButtonColor: '#fe4365',
            confirmButtonText: 'Confirm'
        }).then((result) => {
            if (result.isConfirmed) {
                //Packages the received variable
