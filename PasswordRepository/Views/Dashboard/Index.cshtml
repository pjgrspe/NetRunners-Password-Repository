                                                @*<a class="menu-item green" onclick="toggleEdit('@PassEntry.PID')"> <i class="fa fa-pen"></i> </a>*@
                                                <a class="menu-item green" onclick="updateEntry('@PassEntry.PID','@PassEntry.PID.ToString()')"> <i class="fa fa-pen"></i> </a>
                                            </nav>






                                           
                                            <script type="text/javascript">
                                                /*JASON*/
                                                function toggleshowPW_table() {
                                                    var show = document.getElementById('Username-@PassEntry.PID.ToString()');
                                                    if (show.classList == 'form-control-plaintext') {
                                                        //THIS IS FOR THE EDIT BUTTON
                                                        document.getElementById('staticPassword').readOnly=false;
