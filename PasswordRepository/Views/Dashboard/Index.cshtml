</head>
<body>
    <!--Navbar-->
    <nav class="navbar navbar-expand-lg navbar-dark">
        <div class="container" id="navcontainer">
            <!--Navbar Logo-->
            <a href="/Home/Index" class="navbar-brand">
                <img src="~/Elements/ICElogo200pixel.png" width="150" height="67" alt="">
            </a>
            <!--Navbar Hamburger Menu-->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navmenu">
                <span class="navbar-toggler-icon"></span>
            </button>
            <!--Navbar Items-->
            <div class="collapse navbar-collapse" id="navmenu">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a href="/Home/Index" class="nav-link nav-link-normal" id="navhome">HOME</a>
                    </li>
                    <li class="nav-item">
                        <a href="#team" class="nav-link nav-link-normal" id="navteam">TEAM</a>
                    </li>
                    <li class="nav-item">
                        <a href="#contact" class="nav-link nav-link-normal" id="navcontact">CONTACT</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link nav-link-dropdown" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false" style="text-transform:uppercase">
                            
                            @*USERNAME DISPlAY FOR THE RESPECTIVE USERID*@
                            @Session["Username"].ToString()
                        
                        </a>
                        <ul class="dropdown-menu dropdown-menu-dark dropdown-menu-end" aria-labelledby="navbarDarkDropdownMenuLink">
                            <li><a class="dropdown-item" href="@Url.Action("TrashboardTEST", "Dashboard")">Trash</a></li>
                            <li><a class="dropdown-item" href="#">Settings</a></li>
                            <li><a class="dropdown-item" href="@Url.Action("LogOut", "Login")">Logout</a></li>
                        </ul>
                    </li>
                </ul>
            </div>

        </div>
    </nav>

    <div id="outer">
        <!-- Modal -->
        <div class="modal fade" id="addbutton-modal" tabindex="-1" aria-labelledby="ModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <h1 class="modal-title fs-5" id="ModalLabel">ADD NEW ITEM</h1>
                        <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <!--Form-->
                    <div class="modal-body modal-xl" style="margin-top:-10px;">
                        @using (@Html.BeginForm("Entry", "Dashboard", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            <div class="registration-form container d-flex justify-content-center" style="text-align:center;">
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%;">
                                        @Html.TextBoxFor(Model => Model.textbox_PR_TITLE, new { @class = "form-control rounded-0 w-100", id = "floatingTitle", placeholder = "Title", required = "required" })
                                        <label for="floatingTitle">Title</label>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%; ">
                                        @Html.TextBoxFor(Model => Model.textbox_PR_EMAIL, new { @class = "form-control rounded-0 w-100", id = "floatingEmail", type = "email", placeholder = "Email" })
                                        <label for="floatingEmail">Email</label>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%;">
                                        @Html.TextBoxFor(Model => Model.textbox_PR_USERNAME, new { @class = "form-control rounded-0 w-100", id = "floatingUsername", placeholder = "Username" })
                                        <label for="floatingUsername">Username</label>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%;">
                                        @Html.PasswordFor(Model => Model.textbox_PR_PASSWORD, new { @class = "form-control rounded-0 w-100", id = "floatingPassword-login", placeholder = "Password", required = "required" })
                                        <label for="floatingPassword-login">Password</label>
                                        <span>
                                            <!--Eye icon-->
                                            <img src="~/Elements/eye.svg" id="eyeicon-login" />
                                        </span>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%;">
                                        @Html.TextBoxFor(Model => Model.textbox_PR_URL, new { @class = "form-control rounded-0 w-100", id = "floatingURL", placeholder = "URL" })
                                        <label for="floatingURL">URL</label>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-center w-100">
                                    <div class="form-floating" style="margin-top: 10px; width: 90%; height: auto;">
                                        @Html.TextAreaFor(Model => Model.textbox_PR_NOTES, new { @class = "form-control rounded-0 w-100", rows = "10", id = "floatingNotes", placeholder = "Notes", style = "height:110px", maxlength = "500" })
                                        <label for="floatingNotes">Notes</label>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer d-flex justify-content-center" style="margin-top:10px">
                                <div class="d-flex justify-content-center">
                                    <div class="d-flex justify-content-center col-5 mb-2 ">
                                        <button class="btn btn-outline-secondary rounded-0" aria-pressed="true" id="addmodalbtn">
                                            ADD +
                                        </button>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
        <!--Content-->
        <section class="text-light p-5">
            <div class="container d-flex justify-content-center" id="maincontainer">
                <div class="justify-content-center col-12" id="content">
                    <!--Title-->
                    <div class="d-flex justify-content-center col-12" id="title" style="display:block; white-space:nowrap; margin-left:auto; margin-right:auto;">
                        <h2 align="center">
                            / PASSWORD REPOSITORY /
                        </h2>
                    </div>
                    <!--TABLE-->
                    <div class="d-flex justify-content-center" style="margin-top:20px">
                        <div class="row w-75" id="passrepotable">
                            <!--ITEMS-->
                            <div class="col-4" id="table-leftcolumn">
                                <!--Search bar-->
                                <div class="d-flex justify-content-center">
                                    <a style="width:95%">
                                        <div class="input-group" style="margin-top:10px;">
                                            <span class="input-group-text rounded-0" id="basic-addon1"><img id="searchicon" src="~/Elements/searchicon.png" /></span>
                                            <input type="text" class="form-control form-control-search rounded-0" placeholder="SEARCH" aria-label="Username" aria-describedby="basic-addon1" id="searchform">
                                        </div>
                                    </a>
                                </div>
                                <!-- Button trigger modal -->
                                <div class="d-flex justify-content-center">
                                    <button type="button" class="btn btn-outline-secondary rounded-0" data-bs-toggle="modal" data-bs-target="#addbutton-modal" id="additembtn" style="margin-top:10px;">
                                        ADD ITEM +
                                    </button>
                                </div>
                                <div class="list-group rounded-0 overflow-auto flipped scroller" id="list-tab" role="tablist" style="margin-top:10px; height:500px;">
                                    @{foreach (var PassEntry in Model.Passwords.Where(x => x.UID.Equals((int)Session["ID"]) && x.isTrashed.Equals(false) && x.isActive.Equals(true)))
                                        {
                                            <a class="list-group-item list-group-item-action" id="list-@PassEntry.PID-list" data-bs-toggle="list" href="#list-@PassEntry.PID" role="tab" aria-controls="list-@PassEntry.PID">@PassEntry.PR_TITLE</a>
                                        }
                                    }
                                </div>
                            </div>
                            <!--ITEM DETAILS-->
                            <div class="col-8" id="table-rightcolumn">
                                <div class="tab-content" id="nav-tabContent">
                                    @{foreach (var PassEntry in Model.Passwords.Where(x => x.UID.Equals((int)Session["ID"]) && x.isTrashed.Equals(false) && x.isActive.Equals(true)))
                                        {
                                            var decryptedPassword = Encrypter.DecryptString(PassEntry.PR_PASSWORD);
                                            <!--ITEM 1 DETAILS-->
                                            <div class="tab-pane show" id="list-@PassEntry.PID" role="tabpanel" aria-labelledby="list-@PassEntry.PID-list">
                                                <div class="container">
                                                    <!--TITLE NAME-->
                                                    <div class="mt-3 item-table w-100">
                                                        <h2 class="display-5">
                                                            @PassEntry.PR_TITLE
                                                        </h2>
                                                        <!--USERNAME INPUT-->
                                                        <div class="mt-3 tbl-details">
                                                            <div class="form-floating table-form">
                                                                <p style="margin-bottom:-5px;">Username</p>
                                                                @*<input type="text" readonly class="form-control-plaintext" style="width:100%" id="staticUsername" value="@PassEntry.PR_USERNAME">*@
                                                                @Html.TextBoxFor(Model => Model.textbox_PR_USERNAME, new { @class = "form-control-plaintext", id = "staticUsername", type = "text", @readonly = "readonly", Value = @PassEntry.PR_USERNAME, style = "width:100%" })
                                                                <span>
                                                                    <!--CC icon-->
                                                                    <img src="~/Elements/copytoclipboard.svg" id="ccicon-username" />
                                                                </span>
                                                                <script type="text/javascript">
                                                                    let cciconusername = document.getElementById('ccicon-username');
                                                                    var formdisplayusername = document.getElementById('staticUsername');


                                                                    cciconusername.style.display = "none";

                                                                    formdisplayusername.addEventListener("mouseenter", function () {
                                                                        cciconusername.style.display = "block";
                                                                        formdisplayusername.classList.add("hover");
                                                                    });

                                                                    formdisplayusername.addEventListener("mouseleave", function () {
                                                                        const isTargetIcon = event.relatedTarget === cciconusername || cciconusername.contains(event.relatedTarget);

                                                                        if (!isTargetIcon) {
                                                                            cciconusername.style.display = "none";
                                                                        }
                                                                    });

                                                                    cciconusername.addEventListener("mouseenter", function () {
                                                                        formdisplayusername.classList.add("hover");
                                                                    });

                                                                    cciconusername.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleccUN_table();
                                                                    });

                                                                    function toggleccUN_table() {
                                                                        var copyText = formdisplayusername

                                                                        // Copy the text inside the text field
                                                                        navigator.clipboard.writeText(copyText.value);

                                                                        // Alert the copied text
                                                                        alert("Copied the text: " + copyText.value);
                                                                        cciconusername.style.display = "none";
                                                                    }
                                                                </script>
                                                            </div>
                                                        </div>
                                                        <!--EMAIL INPUT-->
                                                        <div class="mt-3 tbl-details">
                                                            <div class="form-floating table-form">
                                                                <p style="margin-bottom:-5px;">Email</p>
                                                                @*<input type="email" readonly class="form-control-plaintext" style="width:100%" id="staticEmail" value="@PassEntry.PR_EMAIL">*@
                                                                @Html.TextBoxFor(Model => Model.textbox_PR_EMAIL, new { @class = "form-control-plaintext", id = "staticEmail", type = "text", @readonly = "readonly", Value = @PassEntry.PR_EMAIL, style = "width:100%" })
                                                                <span>
                                                                    <!--CC icon-->
                                                                    <img src="~/Elements/copytoclipboard.svg" id="ccicon-email" />
                                                                </span>
                                                                <script type="text/javascript">
                                                                    let cciconemail = document.getElementById('ccicon-email');
                                                                    var formdisplayemail = document.getElementById('staticEmail');


                                                                    cciconemail.style.display = "none";

                                                                    formdisplayemail.addEventListener("mouseenter", function () {
                                                                        cciconemail.style.display = "block";
                                                                        formdisplayemail.classList.add("hover");
                                                                    });

                                                                    formdisplayemail.addEventListener("mouseleave", function () {
                                                                        const isTargetIcon = event.relatedTarget === cciconemail || cciconemail.contains(event.relatedTarget);

                                                                        if (!isTargetIcon) {
                                                                            cciconemail.style.display = "none";
                                                                        }
                                                                    });

                                                                    cciconemail.addEventListener("mouseenter", function () {
                                                                        formdisplay.classList.add("hover");
                                                                    });

                                                                    cciconemail.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleccEMAIL_table();
                                                                    });

                                                                    function toggleccEMAIL_table() {
                                                                        var copyTextemail = formdisplayemail

                                                                        // Copy the text inside the text field
                                                                        navigator.clipboard.writeText(copyTextemail.value);

                                                                        // Alert the copied text
                                                                        alert("Copied the text: " + copyTextemail.value);
                                                                        cciconemail.style.display = "none";
                                                                    }
                                                                </script>
                                                            </div>
                                                        </div>
                                                        <!--PASSWORD INPUT-->
                                                        <div class="mt-3 tbl-details">
                                                            <div class="form-floating table-form">
                                                                <p style="margin-bottom:-5px;">Password</p>
                                                                @*<input type="password" readonly class="form-control-plaintext" style="width:100%" id="staticPassword" value="@decryptedPassword">*@
                                                                @Html.PasswordFor(Model => Model.textbox_PR_PASSWORD, new { @class = "form-control-plaintext", id = "staticPassword", type = "password", required = "required", @readonly = "readonly", Value = @decryptedPassword, style = "width:100%" })
                                                                <span>
                                                                    <!--Eye and CC icon-->
                                                                    <img src="~/Elements/eye-white.svg" id="eyeicon-tbl-password" />
                                                                    <img src="~/Elements/copytoclipboard.svg" id="ccicon-password" />
                                                                </span>
                                                                <!--LUIS-->
                                                                <!--needs a way to call unique IDs depending on what list is active (for both eyeicontblpassword and staticPassword)-->
                                                                <!--rn the ids only applies to the first list-->
                                                                <script type="text/javascript">
                                                                    let eyeicontblpassword = document.getElementById('eyeicon-tbl-password');
                                                                    let cciconpassword = document.getElementById('ccicon-password');
                                                                    var formdisplaypassword = document.getElementById('staticPassword');


                                                                    eyeicontblpassword.style.display = "block";
                                                                    cciconpassword.style.display = "none";

                                                                    formdisplaypassword.addEventListener("mouseenter", function () {
                                                                        cciconpassword.style.display = "block";
                                                                        formdisplaypassword.classList.add("hover");
                                                                    });

                                                                    formdisplaypassword.addEventListener("mouseleave", function () {
                                                                        const isTargetIcon = event.relatedTarget === cciconpassword || cciconpassword.contains(event.relatedTarget);

                                                                        if (!isTargetIcon) {
                                                                            cciconpassword.style.display = "none";
                                                                        }
                                                                    });

                                                                    eyeicontblpassword.addEventListener("mouseenter", function () {
                                                                        formdisplaypassword.classList.add("hover");
                                                                    });

                                                                    cciconpassword.addEventListener("mouseenter", function () {
                                                                        formdisplaypassword.classList.add("hover");
                                                                    });

                                                                    eyeicontblpassword.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleshowPW_table();
                                                                    });

                                                                    cciconpassword.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleccPW_table();
                                                                    });

                                                                    function toggleccPW_table() {
                                                                        var copyText = formdisplaypassword

                                                                        // Copy the text inside the text field
                                                                        navigator.clipboard.writeText(copyText.value);

                                                                        // Alert the copied text
                                                                        alert("Copied the text: " + copyText.value);
                                                                        cciconpassword.style.display = "none";
                                                                    }

                                                                    function toggleshowPW_table() {
                                                                        var show = document.getElementById('staticPassword');
                                                                        if (show.type == 'password') {
                                                                            //THIS IS FOR THE EDIT BUTTON
                                                                            //document.getElementById('staticPassword').readOnly=false;
                                                                            //document.getElementById('staticPassword').className = "form-control";
                                                                            show.type = 'text';
                                                                            eyeicontblpassword.src = "/Elements/eye-slash-white.svg"
                                                                        }
                                                                        else {
                                                                            show.type = 'password'
                                                                            //THIS IS FOR THE EDIT BUTTON
                                                                            //document.getElementById('staticPassword').readOnly=true;
                                                                            //document.getElementById('staticPassword').className = "form-control-plaintext";
                                                                            eyeicontblpassword.src = "/Elements/eye-white.svg"

                                                                        }
                                                                        show.focus();
                                                                    }
                                                                </script>

                                                            </div>
                                                        </div>
                                                        <!--URL INPUT-->
                                                        <div class="mt-3 tbl-details">
                                                            <div class="form-floating table-form">
                                                                <p style="margin-bottom:-5px;">URL</p>
                                                                @*<input type="text" readonly class="form-control-plaintext" style="width:100%" id="staticURL" value="@PassEntry.PR_URL">*@
                                                                @Html.TextBoxFor(Model => Model.textbox_PR_URL, new { @class = "form-control-plaintext", id = "staticURL", type = "text", @readonly = "readonly", Value = @PassEntry.PR_URL, style = "width:100%" })
                                                                <span>
                                                                    <!--CC icon-->
                                                                    <img src="~/Elements/copytoclipboard.svg" id="ccicon-URL" />
                                                                </span>
                                                                <script type="text/javascript">
                                                                    let cciconURL = document.getElementById('ccicon-URL');
                                                                    var formdisplayURL = document.getElementById('staticURL');


                                                                    cciconURL.style.display = "none";

                                                                    formdisplayURL.addEventListener("mouseenter", function () {
                                                                        cciconURL.style.display = "block";
                                                                        formdisplayURL.classList.add("hover");
                                                                    });

                                                                    formdisplayURL.addEventListener("mouseleave", function () {
                                                                        const isTargetIcon = event.relatedTarget === cciconURL || cciconURL.contains(event.relatedTarget);

                                                                        if (!isTargetIcon) {
                                                                            cciconURL.style.display = "none";
                                                                        }
                                                                    });

                                                                    cciconURL.addEventListener("mouseenter", function () {
                                                                        formdisplay.classList.add("hover");
                                                                    });

                                                                    cciconURL.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleccURL_table();
                                                                    });

                                                                    function toggleccURL_table() {
                                                                        var copyTextemail = formdisplayURL

                                                                        // Copy the text inside the text field
                                                                        navigator.clipboard.writeText(copyTextemail.value);

                                                                        // Alert the copied text
                                                                        alert("Copied the text: " + copyTextemail.value);
                                                                        cciconURL.style.display = "none";
                                                                    }
                                                                </script>
                                                            </div>
                                                        </div>
                                                        <!--NOTES INPUT-->
                                                        <!--LUIS-->
                                                        <div class="mt-3 tbl-details">
                                                            <div class="form-floating table-form">
                                                                <p style="margin-bottom:-5px;">Notes</p>
                                                                <textarea type="text" style="text-align: left; height: 100px; width: 100%" readonly class="form-control-plaintext" id="staticNotes">@PassEntry.PR_NOTES</textarea>
                                                                <!--For some reason the html helper doesn't display the "Value = @@PassEntry.PR_NOTES"-->
                                                                @*@Html.TextAreaFor(Model => Model.textbox_PR_NOTES, new { @class = "form-control-plaintext", id = "staticNotes", @readonly = "readonly", type = "text", Value = @PassEntry.PR_NOTES, style = "width:100%; text-align: left; height: 100px;" })*@
                                                                <span>
                                                                    <!--CC icon-->
                                                                    <img src="~/Elements/copytoclipboard.svg" id="ccicon-notes" />
                                                                </span>
                                                                <script type="text/javascript">
                                                                    let cciconnotes = document.getElementById('ccicon-notes');
                                                                    var formdisplaynotes = document.getElementById('staticNotes');


                                                                    cciconnotes.style.display = "none";

                                                                    formdisplaynotes.addEventListener("mouseenter", function () {
                                                                        cciconnotes.style.display = "block";
                                                                        formdisplaynotes.classList.add("hover");
                                                                    });

                                                                    formdisplaynotes.addEventListener("mouseleave", function () {
                                                                        const isTargetIcon = event.relatedTarget === cciconnotes || cciconnotes.contains(event.relatedTarget);

                                                                        if (!isTargetIcon) {
                                                                            cciconnotes.style.display = "none";
                                                                        }
                                                                    });

                                                                    cciconnotes.addEventListener("mouseenter", function () {
                                                                        formdisplay.classList.add("hover");
                                                                    });

                                                                    cciconnotes.addEventListener("mousedown", function (event) {
                                                                        event.preventDefault(); // prevent default action to avoid losing focus
                                                                        toggleccNOTES_table();
                                                                    });

                                                                    function toggleccNOTES_table() {
                                                                        var copyTextemail = formdisplaynotes

                                                                        // Copy the text inside the text field
                                                                        navigator.clipboard.writeText(copyTextemail.value);

                                                                        // Alert the copied text
                                                                        alert("Copied the text: " + copyTextemail.value);
                                                                        cciconnotes.style.display = "none";
                                                                    }
                                                                </script>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--DATE INPUT-->
                                                    <div class="col-12 d-flex justify-content-center">
                                                        <div class="mt-4" style="color:gray; margin-right:60px;">
                                                            <p>Created on @PassEntry.ENTRY_CREATED</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    }
                                    <nav class="menu">
                                        <input type="checkbox" href="#" class="menu-open" name="menu-open" id="menu-open" />
                                        <label class="menu-open-button" for="menu-open">
                                            <span class="lines line-1"></span>
                                            <span class="lines line-2"></span>
                                            <span class="lines line-3"></span>
                                        </label>

                                        <a href="#" class="menu-item red"> <i class="fa fa-trash"></i> </a>
                                        <a class="menu-item green" id="edit-icon"> <i class="fa fa-pen"></i> </a>
                                    </nav>
                                    <!--still fixing-->
                                    @*<script type="text/javascript">
                                        let editicon = document.getElementById('edit-icon');

                                        editicon.addEventListener("mousedown", function (event) {
                                            toggleedit();
                                        });

                                        function toggleedit() {
                                            var show = document.getElementById('staticUsername');
                                            //THIS IS FOR THE EDIT BUTTON
                                            document.getElementById('staticPassword').readOnly = false;
                                            document.getElementById('staticPassword').className = "form-control";
                                            show.focus();
                                        }
                                    </script>*@
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>
</html>