                        }
                    })
                    Swal.fire({
                        position: 'center',
                        icon: 'success',
                        background: '#1a1a1a',
                        iconColor: '#70cc72',
                        title: '"' + title + '" successfuly updated!',
                        showConfirmButton: false,
                        timer: 2500
                    })
                        .then(function () {
                            location.reload(true);
                        }
                        )
                }
            })
        }
    }
